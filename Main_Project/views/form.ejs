<script type="text/javascript">
  // $("#Check1").val("FALSE");

function onCheckChange1() {
    if ($("#Check1").is(':checked')) {
      $("#choice1").removeAttr('disabled');
       
        $("#Check1").val("TRUE");
    } else {
         $("#choice1").attr('disabled', 'disabled');
         $("#choice1").val("0");
        $("#Check1").val("FALSE");
    }
}

function onCheckChange2() {
    if ($("#Check2").is(':checked')) {
      $("#choice2").removeAttr('disabled');
       
        $("#Check2").val("TRUE");
    } else {
         $("#choice2").attr('disabled', 'disabled');
         $("#choice1").val("0");
        $("#Check2").val("FALSE");
    }
}

function onCheckChange3() {
    if ($("#Check3").is(':checked')) {
      $("#choice3").removeAttr('disabled');
       
        $("#Check3").val("TRUE");
    } else {
         $("#choice3").attr('disabled', 'disabled');
         $("#choice1").val("0");
        $("#Check3").val("FALSE");
    }
}

</script>
